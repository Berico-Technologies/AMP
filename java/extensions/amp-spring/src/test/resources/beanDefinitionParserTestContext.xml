<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:amp="http://www.bericotechnologies.com/schema/amp"
	xmlns:jaxbs="http://www.bericotechnologies.com/schema/jaxbs"
	xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.bericotechnologies.com/schema/amp http://www.bericotechnologies.com/schema/amp/amp.xsd
	http://www.bericotechnologies.com/schema/jaxbs http://www.bericotechnologies.com/schema/jaxbs/jaxbs.xsd">
	
	<!-- A typical example, using defaults for all the other values -->
	<amp:fallback id="fallbackTest1" host="devexample.com" exchange="test-exchange" durable="true" />
	
	<!-- None of these are defaults -->
	<amp:fallback id="fallbackTest2" 
		exchange="test-exchange" 
		host="devexample.com" 
		vhost="test"
		port="12345"
		clientName="me"
		exchange-type="topic"
		queue="myqueue"
		durable="true"
		auto-delete="true" />
	
</beans>